const npcDescription = generateNPC();
console.log(npcDescription);

function generateNPC() {
  // Call the selectRace(), selectAge(), selectIntelligence(), selectHair(), and selectStandout() functions to get random values for the NPC's race, age, intelligence, hair, and standout description
  const race = selectRace();
  const age = selectAge();
  const intelligence = selectIntelligence();
  const hair = selectHair();
  const standout = selectStandout();
  const build = selectBuild();
  
  // Generate random first and last names for the NPC
  const firstName = generateRandomName();
  const lastName = generateRandomLastName();

  // Generate random wants and needs for the NPC
  const wantsOrNeeds = generateRandomWantsAndNeeds();

  // Generate a random secret", " is obstacle for the NPC
  const secretOrObstacle = generateRandomSecretOrObstacle();

  // Generate a random list of items that the NPC is carrying
  const alsoCarrying = generateRandomAlsoCarrying();
  // Generate random amounts of gold, silver, and copper
  const gold = Math.floor(Math.random() * 100);
  const silver = Math.floor(Math.random() * 100);
  const copper = Math.floor(Math.random() * 100);

  // Use the random values to create an NPC description
  const description = `${race} named ${firstName} ${lastName}. Appears to be ${age}. They seem ${intelligence}, have ${hair} and ${build}  ${firstName} ${standout}, currently ${wantsOrNeeds}, and ${secretOrObstacle} They have ${alsoCarrying}, ${gold}GP, ${silver}SP, and ${copper}CP on them.`;

  return description;
}

function selectRace() {
  // Array of possible races
  const races = ["An Aarakocra", "An Aasimar", "A Bugbear", "A Centaur", "A Changeling", "A Dragonborn", "A Dwarf", "An Elf", "A Firbolg", "A Genasi", "A Gith", "A Gnome", "A Goblin", "A Goliath", "A Half-Elf", "A Half-Orc", "A Halfling", "A Hobgoblin", "A Human", "A Kalashtar", "A Kenku", "A Kobold", "A Leonin", "A Lizardfolk", "A Loxodon", "A Minotaur", "An Orc", "A Satyr", "A Shifter", "A Simic Hybrid", "A Tabaxi", "A Tiefling", "A Tortle", "A Triton", "A Vedalken", "A Verdan", "A Warforged", "A Yuan-Ti Pureblood"];

  // Generate a random number between 0 and the length of the races array
  const raceIndex = Math.floor(Math.random() * races.length);

  // Use the random number to get a random race from the races array
  const race = races[raceIndex];

  return race;
}

function selectAge() {
  // Array of possible ages
  const ages = ["a teenager", "a young adult", "an adult", "middle-aged"];

  // Generate a random number between 0 and the length of the ages array
  const ageIndex = Math.floor(Math.random() * ages.length);

  // Use the random number to get a random age from the ages array
  const age = ages[ageIndex];

  return age;
}

function selectIntelligence() {
  // Array of possible intelligences
  const intelligences = ["dumb as dirt", "of average intellect, which ain't great", "street smart, not book smart", "book smart, not street smart", "book, and street smart", "smarter than everyone"];

  // Generate a random number between 0 and the length of the intelligences array
  const intelligenceIndex = Math.floor(Math.random() * intelligences.length);

  // Use the random number to get a random intelligence from the intelligences array
  const intelligence = intelligences[intelligenceIndex];

  return intelligence;
}

function selectHair() {
  // Arrays of possible hair olors, and lengths
  const colors = ['auburn','black','brown','chestnut brown','copper','dark brown','ginger red','golden blond','grey','light blond','medium blond','medium brown','black','brown','light brown','light chestnut brown','medium blond','medium brown','black','brown','strawberry blond','white','greenish','purplish','blueish','medium blond','medium brown','black','brown'];
  const lengths = ['long, wavy','long, straight','long, curly','long, frizzy','wavy, shoulder-length','straight, shoulder-length','curly, shoulder-length','frizzy, shoulder-length','short, frizzy','short, wavy','short, curly','short, crew cut','short, flat top','short-cropped','short, bowl cut','medium-length, bowl cut','balding'];

  // Generate random numbers between 0 and the length of each array
  const colorIndex = Math.floor(Math.random() * colors.length);
  const lengthIndex = Math.floor(Math.random() * lengths.length);

  // Use the random numbers to get a random hair style, color, and length from the corresponding arrays
  const color = colors[colorIndex];
  const length = lengths[lengthIndex];

  return `${length}, ${color} hair`;
}

function selectBuild() {
  // Arrays of possible heights and body types
  const heights = ['tall','short','of average height','very tall','very short'];
  const bodyTypes = ['an average','a muscular','a mildly overweight','an extremely overweight','an athletic','a skinny','a scrawny','a starved','an ample','a beer-bellied','a big-boned','a bloated','a broad','a bulky','a chubby','a curvaceous','a doughy','a dumpy','a fleshy','a full-figured','a heavy-set','a plump','a portly','a potbellied','a pudgy','a squat','a stocky','a stout','a thickset','an angular','a bony','a belicate','a fine-boned','a gangly','a lanky','a lean','a lithe','a narrow','a sinuous','a sleek','a slender','a slight','a slim','a spindly','a svelte','a thin','a underweight','a waif-like','a wispy','an active','a beefy','a brawny','a burly','a defined','a fit','a healthy','a herculean','a hulking','a husky','a limber','a nimble','a powerful','a ripped','a robust','a rugged','a shredded','a sinewy','a solid','a strong','a sturdy','a taut','a toned','a tough','an atrophied','a battered','a brittle','a crippled','a crooked','a decrepit','a deformed','a deteriorating','an emaciated','a feeble','a frail','a gaunt','a haggard','a half-starved','a maimed','a malnourished','a puny','a rickety','a ropy','a runty','a scalded','a sickly','an underfed','a weak','a withered','a barrel-chested','a broad-shouldered','a hourglass-shaped','a limp','a pear-shaped','a pumpkinesque','a rotund','a round','a serpentine','a shapely','a small-waisted','a thick-waisted','a top-heavy'];

  // Generate random numbers between 0 and the length of each array
  const heightIndex = Math.floor(Math.random() * heights.length);
  const bodyTypeIndex = Math.floor(Math.random() * bodyTypes.length);

  // Use the random numbers to get a random height and body type from the corresponding arrays
  const height = heights[heightIndex];
  const bodyType = bodyTypes[bodyTypeIndex];

  return ` has ${bodyType} build, and ${height}, for their race.`;
}

